    "express-session": "^1.15.6",
    "express-winston": "^2.5.1",
    "file-stream-rotator": "~0.2.1",
    "generate-password": "~1.4.0",
    "glob": "~7.1.0",
    "google-auth-library": "^1.6.1",
    "gulp-ava": "^0.19.0",
    "helmet": "~3.12.1",
    "idtoken-verifier": "^1.2.0",
    "jasmine-core": "~3.1.0",
    "jsonwebtoken": "^8.2.2",
    "lodash": "^4.17.10",
    "lusca": "~1.6.0",
    "method-override": "~2.3.5",
