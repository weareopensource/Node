    "pug": "^2.0.3",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "sequelize": "^4.37.10",
    "semver": "~5.5.0",
    "serve-favicon": "^2.5.0",
    "should": "~13.2.1",
    "socket.io": "^2.1.1",
    "supertest": "^3.1.0",
    "swig": "^1.4.2",
    "validator": "~10.3.0",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "async-to-gen": "^1.3.0",
