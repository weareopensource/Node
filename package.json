{
  "name": "meanjs",
  "description": "Full-Stack JavaScript with MongoDB, Express, AngularJS, and Node.js.",
  "version": "2.0.0-backend",
  "meanjs-version": "2.0.0-backend",
  "private": false,
  "author": "https://github.com/meanjs/mean/graphs/contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/meanjs/mean.git"
  },
  "engines": {
    "node": ">=8.4.0",
    "npm": ">=5.3.0"
  },
  "scripts": {
    "update": "npm update && npm prune",
    "clean": "rm -rf node_modules/",
    "reinstall": "npm cache clean && npm run clean && npm install",
    "start": "cross-env NODE_ENV=development node server.js",
    "start:debug": "cross-env NODE_ENV=development nodemon --inspect server.js",
    "prod": "cross-env NODE_ENV=production node server.js ",
    "test": "gulp test",
    "ava": "cross-env NODE_ENV=development node --harmony server.js & sleep 5 && ava",
    "test:server": "gulp test:server",
    "test:server:watch": "gulp test:server:watch",
    "test:coverage": "gulp test:coverage",
    "generate-ssl-certs": "lib/scripts/generate-ssl-certs.sh"
  },
  "dependencies": {
    "acl": "~0.4.9",
    "async": "~2.6.1",
    "bcrypt": "^2.0.1",
