    "gulp-rename": "^1.2.3",
    "gulp-util": "~3.0.7",
    "istanbul": "~0.4.2",
    "lcov-result-merger": "~3.0.0",
    "mock-fs": "~4.5.0",
    "run-sequence": "~2.2.1"
  },
  "ava": {
    "require": [
      "async-to-gen/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
